{
  "sql": [
    {
      "engine": "postgresql",
      "queries": ["internal/user/adapter/postgresAdapter/query"],
      "schema": "pkg/postgres/migration",
      "gen": {
        "go": {
          "package": "postgres",
          "out": "pkg/postgres",
          "emit_prepared_queries": true,
          "emit_exact_table_names": true,
          "emit_interface": true,
          "emit_json_tags": true,
          "emit_enum_valid_method": true,
          "emit_all_enum_values": true,
          "json_tags_case_style": "camel",
          "emit_exported_queries": true,
          "sql_package": "pgx/v5"

        }
      }
    }
  ],
  "version": "2"
}